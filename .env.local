# React environment variables (Loaded by Create React App)
# These should generally match the values in your main .env file,
# but are prefixed with REACT_APP_

# The URL of your backend API
REACT_APP_API_URL=http://localhost:5000/api

# Set to 'development', 'production', or 'test'
REACT_APP_ENV=development

# Replace with your Google Maps API Key (obtained from Google Cloud Console)
# Ensure this key is properly restricted (e.g., to your domain)
REACT_APP_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE

# Replace with your Gemini API Key (obtained from Google AI Studio or Google Cloud)
# Ensure this key is properly restricted
REACT_APP_GEMINI_API_KEY=YOUR_GEMINI_API_KEY_HERE 